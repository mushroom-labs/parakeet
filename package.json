{
  "name": "parakeet",
  "version": "0.0.1",
  "scripts": {
    "_build:compile": "tsc --project ./tsconfig.json",
    "_build:bundle": "browserify ./bin/build/main_client.js -o ./bin/client/build/bundle.js",
    "build": "npm run-script _build:compile && npm run-script _build:bundle",
    "_watch:compile": "tsc --watch --project ./tsconfig.json",
    "_watch:bundle": "watchify ./bin/build/main_client.js -o ./bin/client/build/bundle.js -v -d",
    "watch": "parallel _watch:bundle _watch:compile",
    "start": "cd bin/build && node main_server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mushroom-labs/parakeet.git"
  },
  "author": "Mushroom Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mushroom-labs/parakeet/issues"
  },
  "homepage": "https://github.com/mushroom-labs/parakeet#readme",
  "dependencies": {
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.24",
    "@types/uuid": "^3.4.0",
    "@types/ws": "^3.0.2",
    "browserify": "^14.4.0",
    "express": "^4.15.4",
    "typescript": "^2.5.1",
    "uuid": "^3.1.0",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "npm-runtools": "^1.0.1",
    "ts-node": "^3.3.0",
    "watchify": "^3.9.0"
  }
}
